{
    "mcpServers": {
        "traiq": {
            "type": "http",
            "url": "http://fitapp.test/mcp/workout",
            "headers": {
                "Authorization": "Bearer ${TRAIQ_API_TOKEN}"
            }
        },
        "laravel-boost": {
            "command": "php",
            "args": [
                "artisan",
                "boost:mcp"
            ]
        },
        "herd": {
            "command": "php",
            "args": [
                "/Applications/Herd.app/Contents/Resources/herd-mcp.phar"
            ],
            "env": {
                "SITE_PATH": "/Users/tijmenwierenga/www/tijmenwierenga/fitapp"
            }
        }
    }
}